<!DOCTYPE html>
<html lang="en">
    <head>
        <%include Header.ejs %>
        <title>Query Upload</title>
    </head>
    <body>
        <%include navbar.ejs %>
        <div class="container">
            <div class="progress">
                <div id="pb" class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" style="width: 0%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            
            <form action="submit-form" method="post" enctype="multipart/form-data">
            <input type="file" name="input" upload="multer" multiple="multiple" accept=".avi, .jpg"><br>
            <input type="submit">
            </form>
        </div>
        <%include Footer.ejs %>
    </body>
</html>